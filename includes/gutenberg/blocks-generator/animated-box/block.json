{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "olena/animated-box",
	"version": "0.1.0",
	"title": "Animated Box",
	"category": "widgets",
	"icon": "align-wide",
	"description": "Add an Animated Box",
	"supports": {
		"html": false,
		"anchor": true
	},
	"attributes": {
		"animation_type": {
			"type": "string",
			"source": "attribute",
			"selector": ".wp-block-olena-animated-box",
			"attribute": "data-animation-type",
			"default": "fadeInUp"
		},
		"extras": {
			"type": "string",
			"source": "attribute",
			"selector": ".wp-block-olena-animated-box",
			"attribute": "data-extras",
			"default": ""
		}
	},
	"textdomain": "olena",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}